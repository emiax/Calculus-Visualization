    },
    translate: function (a, b, c) {
        var d = b[0],
            f = b[1],
            b = b[2];
        if (!c || a == c) return a[12] = a[0] * d + a[4] * f + a[8] * b + a[12], a[13] = a[1] * d + a[5] * f + a[9] * b + a[13], a[14] = a[2] * d + a[6] * f + a[10] * b + a[14], a[15] = a[3] * d + a[7] * f + a[11] * b + a[15], a;
