                top: b,
                left: c
            }
        },
        setOffset: function (a, b, c) {
            var d = e.css(a, "position");
            if (d === "static") a.style.position = "relative";
            var f = e(a),
                g = f.offset(),
                h = e.css(a, "top"),
                i = e.css(a, "left"),
                j = {}, l = {};
