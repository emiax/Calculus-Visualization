        b[1] = (-i * d + f * q) * v;
        b[2] = (j * d - f * g) * v;
        b[3] = u * v;
        b[4] = (i * c - f * o) * v;
        b[5] = (-j * c + f * h) * v;
        b[6] = l * v;
        b[7] = (-q * c + d * o) * v;
        b[8] = (g * c - d * h) * v;
        return b
    },
    multiply: function (a, b, c) {
        c || (c = a);
        var d = a[0],
            f = a[1],
            h = a[2],
            g = a[3],
            j = a[4],
            o = a[5],
            q = a[6],
            i = a[7],
            r = a[8],
            u = a[9],
            l = a[10],
            v = a[11],
            z = a[12],
            G = a[13],
            y = a[14],
