    toInverseMat3: function (a, b) {
        var c = a[0],
            d = a[1],
            f = a[2],
            h = a[4],
            g = a[5],
            j = a[6],
            o = a[8],
            q = a[9],
            i = a[10],
            r = i * g - j * q,
            u = -i * h + j * o,
            l = q * h - g * o,
            v = c * r + d * u + f * l;
