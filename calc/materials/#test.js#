        c[13] = g * d + i * f + v * b + a[13];
        c[14] = j * d + r * f + z * b + a[14];
        c[15] = o * d + u * f + G * b + a[15];
        return c
    },
    scale: function (a, b, c) {
        var d = b[0],
            f = b[1],
            b = b[2];
        if (!c || a == c) return a[0] *= d, a[1] *= d, a[2] *= d, a[3] *= d, a[4] *= f, a[5] *= f, a[6] *= f, a[7] *= f, a[8] *= b, a[9] *= b, a[10] *= b, a[11] *= b, a;
