                    j = j + (parseFloat(c.borderLeftWidth) || 0)
                }
                d = b.offsetParent
            }
            if (e.support.subtractsBorderForOverflowNotVisible && c.overflow !== "visible") {
                i = i + (parseFloat(c.borderTopWidth) || 0);
                j = j + (parseFloat(c.borderLeftWidth) || 0)
            }
        }
        if (c.position === "relative" || c.position === "static") {
            i = i + h.offsetTop;
            j = j + h.offsetLeft
        }
        if (e.support.fixedPosition && c.position === "fixed") {
            i = i + Math.max(g.scrollTop, h.scrollTop);
            j = j + Math.max(g.scrollLeft, h.scrollLeft)
        }
        return {
            top: i,
            left: j
        }
    };
    e.offset = {
        bodyOffset: function (a) {
