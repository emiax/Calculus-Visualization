        c[1] = a[1] * d;
        c[2] = a[2] * d;
        c[3] = a[3] * d;
        c[4] = a[4] * f;
        c[5] = a[5] * f;
        c[6] = a[6] * f;
        c[7] = a[7] * f;
        c[8] = a[8] * b;
        c[9] = a[9] * b;
        c[10] = a[10] * b;
        c[11] = a[11] * b;
        c[12] = a[12];
        c[13] = a[13];
        c[14] = a[14];
        c[15] = a[15];
        return c
    },
    rotate: function (a, b, c, d) {
        var f = c[0],
            h = c[1],
            c = c[2],
            g = Math.sqrt(f * f + h * h + c * c);
        if (!g) return null;
        1 != g && (g = 1 / g, f *= g, h *= g, c *= g);
        var j = Math.sin(b),
            o = Math.cos(b),
            q = 1 - o,
            b = a[0],
            g = a[1],
            i = a[2],
            r = a[3],
            u = a[4],
            l = a[5],
            v = a[6],
            z = a[7],
            G = a[8],
            y = a[9],
            A = a[10],
            B = a[11],
            C = f * f * q + o,
            w = h * f * q + c * j,
            D = c * f * q - h * j,
            H = f * h * q - c * j,
            E = h * h * q + o,
            s = c * h * q + f * j,
            F = f * c * q + h * j,
            f = h * c * q - f * j,
            h = c * c * q + o;
        d ? a != d && (d[12] = a[12], d[13] = a[13], d[14] = a[14], d[15] = a[15]) : d = a;
        d[0] = b * C + u * w + G * D;
        d[1] = g * C + l * w + y * D;
        d[2] = i * C + v * w + A * D;
        d[3] = r * C + z * w + B * D;
        d[4] = b * H + u * E + G * s;
        d[5] = g * H + l * E + y * s;
        d[6] = i * H + v * E + A * s;
        d[7] = r * H + z * E + B * s;
        d[8] = b * F + u * f + G * h;
        d[9] = g * F + l * f + y * h;
        d[10] = i * F + v * f + A * h;
        d[11] = r * F + z * f + B * h;
        return d
    },
    rotateX: function (a, b, c) {
        var d = Math.sin(b),
            b = Math.cos(b),
            f = a[4],
            h = a[5],
            g = a[6],
            j = a[7],
            o = a[8],
            q = a[9],
            i = a[10],
            r = a[11];
        c ? a != c && (c[0] = a[0], c[1] = a[1], c[2] = a[2], c[3] = a[3], c[12] = a[12], c[13] = a[13], c[14] = a[14], c[15] = a[15]) : c = a;
        c[4] = f * b + o * d;
        c[5] = h * b + q * d;
        c[6] = g * b + i * d;
        c[7] = j * b + r * d;
        c[8] = f * -d + o * b;
        c[9] = h * -d + q * b;
        c[10] = g * -d + i * b;
        c[11] = j * -d + r * b;
        return c
    },
    rotateY: function (a, b, c) {
        var d = Math.sin(b),
            b = Math.cos(b),
            f = a[0],
            h = a[1],
            g = a[2],
            j = a[3],
            o = a[8],
            q = a[9],
            i = a[10],
            r = a[11];
        c ? a != c && (c[4] = a[4], c[5] = a[5], c[6] = a[6], c[7] = a[7], c[12] = a[12], c[13] = a[13], c[14] = a[14], c[15] = a[15]) : c = a;
        c[0] = f * b + o * -d;
        c[1] = h * b + q * -d;
        c[2] = g * b + i * -d;
        c[3] = j * b + r * -d;
        c[8] = f * d + o * b;
        c[9] = h * d + q * b;
        c[10] = g * d + i * b;
        c[11] = j * d + r * b;
        return c
    },
    rotateZ: function (a, b, c) {
        var d = Math.sin(b),
            b = Math.cos(b),
            f = a[0],
            h = a[1],
            g = a[2],
            j = a[3],
            o = a[4],
            q = a[5],
            i = a[6],
            r = a[7];
        c ? a != c && (c[8] = a[8], c[9] = a[9], c[10] = a[10], c[11] = a[11], c[12] = a[12], c[13] = a[13], c[14] = a[14], c[15] = a[15]) : c = a;
        c[0] = f * b + o * d;
        c[1] = h * b + q * d;
        c[2] = g * b + i * d;
        c[3] = j * b + r * d;
        c[4] = f * -d + o * b;
        c[5] = h * -d + q * b;
        c[6] = g * -d + i * b;
        c[7] = j * -d + r * b;
        return c
    },
    frustum: function (a, b, c, d, f, h, g) {
        g || (g = mat4.create());
        var j = b - a,
            o = d - c,
            q = h - f;
        g[0] = 2 * f / j;
        g[1] = 0;
        g[2] = 0;
        g[3] = 0;
        g[4] = 0;
        g[5] = 2 * f / o;
        g[6] = 0;
        g[7] = 0;
        g[8] = (b + a) / j;
        g[9] = (d + c) / o;
        g[10] = -(h + f) / q;
        g[11] = -1;
        g[12] = 0;
        g[13] = 0;
        g[14] = -(2 * h * f) / q;
        g[15] = 0;
        return g
    },
    perspective: function (a, b, c, d, f) {
        a = c * Math.tan(a * Math.PI / 360);
        b *= a;
        return mat4.frustum(-b, b, -a, a, c, d, f)
    },
    ortho: function (a, b, c, d, f, h, g) {
        g || (g = mat4.create());
        var j = b - a,
            o = d - c,
            q = h - f;
        g[0] = 2 / j;
        g[1] = 0;
        g[2] = 0;
        g[3] = 0;
        g[4] = 0;
        g[5] = 2 / o;
        g[6] = 0;
        g[7] = 0;
        g[8] = 0;
        g[9] = 0;
        g[10] = -2 / q;
        g[11] = 0;
        g[12] = -(a + b) / j;
        g[13] = -(d + c) / o;
        g[14] = -(h + f) / q;
        g[15] = 1;
        return g
    },
    lookAt: function (a, b, c, d) {
        d || (d = mat4.create());
        var f = a[0],
            h = a[1],
            a = a[2],
            g = c[0],
            j = c[1],
            o = c[2],
            c = b[1],
            q = b[2];
        if (f == b[0] && h == c && a == q) return mat4.identity(d);
        var i, r, u, l, c = f - b[0],
            q = h - b[1],
            b = a - b[2];
        l = 1 / Math.sqrt(c * c + q * q + b * b);
        c *= l;
        q *= l;
        b *= l;
        i = j * b - o * q;
        o = o * c - g * b;
        g = g * q - j * c;
        (l = Math.sqrt(i * i + o * o + g * g)) ? (l = 1 / l, i *= l, o *= l, g *= l) : g = o = i = 0;
        j = q * g - b * o;
        r = b * i - c * g;
        u = c * o - q * i;
        (l = Math.sqrt(j * j + r * r + u * u)) ? (l = 1 / l, j *= l, r *= l, u *= l) : u = r = j = 0;
        d[0] = i;
        d[1] = j;
        d[2] = c;
        d[3] = 0;
        d[4] = o;
        d[5] = r;
        d[6] = q;
        d[7] = 0;
        d[8] = g;
        d[9] = u;
        d[10] = b;
        d[11] = 0;
        d[12] = -(i * f + o * h + g * a);
        d[13] = -(j * f + r * h + u * a);
        d[14] = -(c * f + q * h + b * a);
        d[15] = 1;
        return d
    },
    str: function (a) {
        return "[" + a[0] + ", " + a[1] + ", " + a[2] + ", " + a[3] + ", " + a[4] + ", " + a[5] + ", " + a[6] + ", " + a[7] + ", " + a[8] + ", " + a[9] + ", " + a[10] + ", " + a[11] + ", " + a[12] + ", " + a[13] + ", " + a[14] + ", " + a[15] + "]"
    }
};
quat4 = {
    create: function (a) {
        var b = new glMatrixArrayType(4);
        a && (b[0] = a[0], b[1] = a[1], b[2] = a[2], b[3] = a[3]);
        return b
    },
    set: function (a, b) {
        b[0] = a[0];
        b[1] = a[1];
        b[2] = a[2];
        b[3] = a[3];
        return b
    },
    calculateW: function (a, b) {
        var c = a[0],
            d = a[1],
            f = a[2];
        if (!b || a == b) return a[3] = -Math.sqrt(Math.abs(1 - c * c - d * d - f * f)), a;
        b[0] = c;
        b[1] = d;
        b[2] = f;
        b[3] = -Math.sqrt(Math.abs(1 - c * c - d * d - f * f));
        return b
    },
    inverse: function (a, b) {
        if (!b || a == b) return a[0] *= -1, a[1] *= -1, a[2] *= -1, a;
        b[0] = -a[0];
        b[1] = -a[1];
        b[2] = -a[2];
        b[3] = a[3];
        return b
    },
    length: function (a) {
        var b = a[0],
            c = a[1],
            d = a[2],
            a = a[3];
        return Math.sqrt(b * b + c * c + d * d + a * a)
    },
    normalize: function (a, b) {
        b || (b = a);
        var c = a[0],
            d = a[1],
            f = a[2],
            h = a[3],
            g = Math.sqrt(c * c + d * d + f * f + h * h);
        if (0 == g) return b[0] = 0, b[1] = 0, b[2] = 0, b[3] = 0, b;
        g = 1 / g;
        b[0] = c * g;
        b[1] = d * g;
        b[2] = f * g;
        b[3] = h * g;
        return b
    },
    multiply: function (a, b, c) {
        c || (c = a);
        var d = a[0],
            f = a[1],
            h = a[2],
            a = a[3],
            g = b[0],
            j = b[1],
            o = b[2],
            b = b[3];
        c[0] = d * b + a * g + f * o - h * j;
        c[1] = f * b + a * j + h * g - d * o;
        c[2] = h * b + a * o + d * j - f * g;
        c[3] = a * b - d * g - f * j - h * o;
        return c
    },
    multiplyVec3: function (a, b, c) {
        c || (c = b);
        var d = b[0],
            f = b[1],
            h = b[2],
            b = a[0],
            g = a[1],
            j = a[2],
            a = a[3],
            o = a * d + g * h - j * f,
            q = a * f + j * d - b * h,
            i = a * h + b * f - g * d,
            d = -b * d - g * f - j * h;
        c[0] = o * a + d * -b + q * -j - i * -g;
        c[1] = q * a + d * -g + i * -b - o * -j;
        c[2] = i * a + d * -j + o * -g - q * -b;
        return c
    },
    toMat3: function (a, b) {
        b || (b = mat3.create());
        var c = a[0],
            d = a[1],
            f = a[2],
            h = a[3],
            g = c + c,
            j = d + d,
            o = f + f,
            q = c * g,
            i = c * j,
            c = c * o,
            r = d * j,
            d = d * o,
            f = f * o,
            g = h * g,
            j = h * j,
            h = h * o;
        b[0] = 1 - (r + f);
        b[1] = i - h;
        b[2] = c + j;
        b[3] = i + h;
        b[4] = 1 - (q + f);
        b[5] = d - g;
        b[6] = c - j;
        b[7] = d + g;
        b[8] = 1 - (q + r);
        return b
    },
    toMat4: function (a, b) {
        b || (b = mat4.create());
        var c = a[0],
            d = a[1],
            f = a[2],
            h = a[3],
            g = c + c,
            j = d + d,
            o = f + f,
            q = c * g,
            i = c * j,
            c = c * o,
            r = d * j,
            d = d * o,
            f = f * o,
            g = h * g,
            j = h * j,
            h = h * o;
        b[0] = 1 - (r + f);
        b[1] = i - h;
        b[2] = c + j;
        b[3] = 0;
        b[4] = i + h;
        b[5] = 1 - (q + f);
        b[6] = d - g;
        b[7] = 0;
        b[8] = c - j;
        b[9] = d + g;
        b[10] = 1 - (q + r);
        b[11] = 0;
        b[12] = 0;
        b[13] = 0;
        b[14] = 0;
        b[15] = 1;
        return b
    },
    slerp: function (a, b, c, d) {
        d || (d = a);
        var f = a[0] * b[0] + a[1] * b[1] + a[2] * b[2] + a[3] * b[3];
        if (1 <= Math.abs(f)) return d != a && (d[0] = a[0], d[1] = a[1], d[2] = a[2], d[3] = a[3]), d;
        var h = Math.acos(f),
            g = Math.sqrt(1 - f * f);
        if (0.001 > Math.abs(g)) return d[0] = 0.5 * a[0] + 0.5 * b[0], d[1] = 0.5 * a[1] + 0.5 * b[1], d[2] = 0.5 * a[2] + 0.5 * b[2], d[3] = 0.5 * a[3] + 0.5 * b[3], d;
        f = Math.sin((1 - c) * h) / g;
        c = Math.sin(c * h) / g;
        d[0] = a[0] * f + b[0] * c;
        d[1] = a[1] * f + b[1] * c;
        d[2] = a[2] * f + b[2] * c;
        d[3] = a[3] * f + b[3] * c;
        return d
    },
    str: function (a) {
        return "[" + a[0] + ", " + a[1] + ", " + a[2] + ", " + a[3] + "]"
    }
};
var TWEEN = TWEEN || function () {
        var a, b, c, d, f = [],
            h = null;
        return {
            start: function (a) {
                c = setInterval(this.update, 1E3 / (a || 60))
            },
            stop: function () {
                clearInterval(c)
            },
            add: function (a) {
                f.push(a)
            },
            getAll: function () {
                return f
            },
            removeAll: function () {
                f = []
            },
            remove: function (b) {
                a = f.indexOf(b); - 1 !== a && f.splice(a, 1)
            },
            update: function () {
                a = 0;
                b = f.length;
                for (d = (new Date).getTime(); a < b;) f[a].update(d) ? a++ : (f.splice(a, 1), b--);
                !f.length && h && (h(), h = null)
            },
            completeAll: function () {
                for (; f.length;) f[0].complete(), f.shift();
                h && (h(), h = null)
            },
            onComplete: function (a) {
                h = a
            }
        }
    }();
TWEEN.Tween = function (a) {
    var b = {}, c = {}, d = {}, f = 1E3,
        h = 0,
        g = null,
        j = TWEEN.Easing.Linear.EaseNone,
        o = null,
        q = null,
        i = null;
    this.to = function (b, c) {
        null !== c && (f = c);
        for (var h in b) null !== a[h] && (d[h] = b[h]);
        return this
    };
    this.start = function () {
        TWEEN.add(this);
        g = (new Date).getTime() + h;
        for (var f in d) null !== a[f] && (b[f] = a[f], c[f] = d[f] - a[f]);
        return this
    };
    this.stop = function () {
        TWEEN.remove(this);
        return this
    };
    this.delay = function (a) {
        h = a;
        return this
    };
    this.easing = function (a) {
        j = a;
        return this
    };
    this.chain = function (a) {
        o = a
    };
    this.onUpdate = function (a) {
        q = a;
        return this
    };
    this.onComplete = function (a) {
        i = a;
        return this
    };
    this.update = function (d) {
        var h, l;
        if (d < g) return !0;
        d = (d - g) / f;
        d = 1 < d ? 1 : d;
        l = j(d);
        for (h in c) a[h] = b[h] + c[h] * l;
        null !== q && q.call(a, l);
        return 1 == d ? (null !== i && i.call(a), null !== o && o.start(), !1) : !0
    };
    this.complete = function () {
        for (property in c) a[property] = b[property] + c[property];
        null !== q && q.call(a, 1);
        null !== i && i.call(a);
        null !== o && o.complete()
    }
};
TWEEN.Easing = {
    Linear: {},
    Quadratic: {},
    Cubic: {},
    Quartic: {},
    Quintic: {},
    Sinusoidal: {},
    Exponential: {},
    Circular: {},
    Elastic: {},
    Back: {},
    Bounce: {}
};
TWEEN.Easing.Linear.EaseNone = function (a) {
    return a
};
TWEEN.Easing.Quadratic.EaseIn = function (a) {
    return a * a
};
TWEEN.Easing.Quadratic.EaseOut = function (a) {
    return -a * (a - 2)
};
TWEEN.Easing.Quadratic.EaseInOut = function (a) {
    return 1 > (a *= 2) ? 0.5 * a * a : -0.5 * (--a * (a - 2) - 1)
};
TWEEN.Easing.Cubic.EaseIn = function (a) {
    return a * a * a
};
TWEEN.Easing.Cubic.EaseOut = function (a) {
    return --a * a * a + 1
};
TWEEN.Easing.Cubic.EaseInOut = function (a) {
    return 1 > (a *= 2) ? 0.5 * a * a * a : 0.5 * ((a -= 2) * a * a + 2)
};
TWEEN.Easing.Quartic.EaseIn = function (a) {
    return a * a * a * a
};
TWEEN.Easing.Quartic.EaseOut = function (a) {
    return -(--a * a * a * a - 1)
};
TWEEN.Easing.Quartic.EaseInOut = function (a) {
    return 1 > (a *= 2) ? 0.5 * a * a * a * a : -0.5 * ((a -= 2) * a * a * a - 2)
};
TWEEN.Easing.Quintic.EaseIn = function (a) {
    return a * a * a * a * a
};
TWEEN.Easing.Quintic.EaseOut = function (a) {
    return (a -= 1) * a * a * a * a + 1
};
TWEEN.Easing.Quintic.EaseInOut = function (a) {
    return 1 > (a *= 2) ? 0.5 * a * a * a * a * a : 0.5 * ((a -= 2) * a * a * a * a + 2)
};
TWEEN.Easing.Sinusoidal.EaseIn = function (a) {
    return -Math.cos(a * Math.PI / 2) + 1
};
TWEEN.Easing.Sinusoidal.EaseOut = function (a) {
    return Math.sin(a * Math.PI / 2)
};
TWEEN.Easing.Sinusoidal.EaseInOut = function (a) {
    return -0.5 * (Math.cos(Math.PI * a) - 1)
};
TWEEN.Easing.Exponential.EaseIn = function (a) {
    return 0 == a ? 0 : Math.pow(2, 10 * (a - 1))
};
TWEEN.Easing.Exponential.EaseOut = function (a) {
    return 1 == a ? 1 : -Math.pow(2, -10 * a) + 1
};
TWEEN.Easing.Exponential.EaseInOut = function (a) {
    return 0 == a ? 0 : 1 == a ? 1 : 1 > (a *= 2) ? 0.5 * Math.pow(2, 10 * (a - 1)) : 0.5 * (-Math.pow(2, -10 * (a - 1)) + 2)
};
TWEEN.Easing.Circular.EaseIn = function (a) {
    return -(Math.sqrt(1 - a * a) - 1)
};
TWEEN.Easing.Circular.EaseOut = function (a) {
    return Math.sqrt(1 - --a * a)
};
TWEEN.Easing.Circular.EaseInOut = function (a) {
    return 1 > (a /= 0.5) ? -0.5 * (Math.sqrt(1 - a * a) - 1) : 0.5 * (Math.sqrt(1 - (a -= 2) * a) + 1)
};
TWEEN.Easing.Elastic.EaseIn = function (a) {
    var b, c = 0.1,
        d = 0.4;
    if (0 == a) return 0;
    if (1 == a) return 1;
    d || (d = 0.3);
    !c || 1 > c ? (c = 1, b = d / 4) : b = d / (2 * Math.PI) * Math.asin(1 / c);
    return -(c * Math.pow(2, 10 * (a -= 1)) * Math.sin((a - b) * 2 * Math.PI / d))
};
TWEEN.Easing.Elastic.EaseOut = function (a) {
    var b, c = 0.1,
        d = 0.4;
    if (0 == a) return 0;
    if (1 == a) return 1;
    d || (d = 0.3);
    !c || 1 > c ? (c = 1, b = d / 4) : b = d / (2 * Math.PI) * Math.asin(1 / c);
    return c * Math.pow(2, -10 * a) * Math.sin((a - b) * 2 * Math.PI / d) + 1
};
TWEEN.Easing.Elastic.EaseInOut = function (a) {
    var b, c = 0.1,
        d = 0.4;
    if (0 == a) return 0;
    if (1 == a) return 1;
    d || (d = 0.3);
    !c || 1 > c ? (c = 1, b = d / 4) : b = d / (2 * Math.PI) * Math.asin(1 / c);
    return 1 > (a *= 2) ? -0.5 * c * Math.pow(2, 10 * (a -= 1)) * Math.sin((a - b) * 2 * Math.PI / d) : 0.5 * c * Math.pow(2, -10 * (a -= 1)) * Math.sin((a - b) * 2 * Math.PI / d) + 1
};
TWEEN.Easing.Back.EaseIn = function (a) {
    return a * a * (2.70158 * a - 1.70158)
};
TWEEN.Easing.Back.EaseOut = function (a) {
    return (a -= 1) * a * (2.70158 * a + 1.70158) + 1
};
TWEEN.Easing.Back.EaseInOut = function (a) {
    return 1 > (a *= 2) ? 0.5 * a * a * (3.5949095 * a - 2.5949095) : 0.5 * ((a -= 2) * a * (3.5949095 * a + 2.5949095) + 2)
};
TWEEN.Easing.Bounce.EaseIn = function (a) {
    return 1 - TWEEN.Easing.Bounce.EaseOut(1 - a)
};
TWEEN.Easing.Bounce.EaseOut = function (a) {
    return (a /= 1) < 1 / 2.75 ? 7.5625 * a * a : a < 2 / 2.75 ? 7.5625 * (a -= 1.5 / 2.75) * a + 0.75 : a < 2.5 / 2.75 ? 7.5625 * (a -= 2.25 / 2.75) * a + 0.9375 : 7.5625 * (a -= 2.625 / 2.75) * a + 0.984375
};
TWEEN.Easing.Bounce.EaseInOut = function (a) {
    return 0.5 > a ? 0.5 * TWEEN.Easing.Bounce.EaseIn(2 * a) : 0.5 * TWEEN.Easing.Bounce.EaseOut(2 * a - 1) + 0.5
};
