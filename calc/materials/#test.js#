    identity: function (a) {
        a[0] = 1;
        a[1] = 0;
        a[2] = 0;
        a[3] = 0;
        a[4] = 0;
        a[5] = 1;
        a[6] = 0;
        a[7] = 0;
        a[8] = 0;
        a[9] = 0;
        a[10] = 1;
        a[11] = 0;
        a[12] = 0;
        a[13] = 0;
        a[14] = 0;
        a[15] = 1;
        return a
    },
    transpose: function (a, b) {
        if (!b || a == b) {
            var c = a[1],
                d = a[2],
                f = a[3],
                h = a[6],
                g = a[7],
                j = a[11];
            a[1] = a[4];
            a[2] = a[8];
            a[3] = a[12];
            a[4] = c;
            a[6] = a[9];
            a[7] = a[13];
            a[8] = d;
            a[9] = h;
            a[11] = a[14];
            a[12] = f;
            a[13] = g;
            a[14] = j;
            return a
        }
        b[0] = a[0];
        b[1] = a[4];
        b[2] = a[8];
        b[3] = a[12];
        b[4] = a[1];
        b[5] = a[5];
        b[6] = a[9];
        b[7] = a[13];
        b[8] = a[2];
        b[9] = a[6];
        b[10] = a[10];
