        b[12] = a[3];
        b[13] = a[7];
        b[14] = a[11];
        b[15] = a[15];
        return b
    },
    determinant: function (a) {
        var b = a[0],
            c = a[1],
            d = a[2],
            f = a[3],
            h = a[4],
            g = a[5],
            j = a[6],
            o = a[7],
            q = a[8],
            i = a[9],
            r = a[10],
            u = a[11],
            l = a[12],
            v = a[13],
            z = a[14],
            a = a[15];
        return l * i * j * f - q * v * j * f - l * g * r * f + h * v * r * f + q * g * z * f - h * i * z * f - l * i * d * o + q * v * d * o + l * c * r * o - b * v * r * o - q * c * z * o + b * i * z * o + l * g * d * u - h * v * d * u - l * c * j * u + b * v * j * u + h * c * z * u - b * g * z * u - q * g * d * a + h * i * d * a + q * c * j * a - b * i * j * a - h * c * r * a + b * g * r * a
    },
    inverse: function (a, b) {
        b || (b = a);
        var c = a[0],
            d = a[1],
            f = a[2],
            h = a[3],
            g = a[4],
            j = a[5],
            o = a[6],
            q = a[7],
            i = a[8],
            r = a[9],
            u = a[10],
            l = a[11],
            v = a[12],
            z = a[13],
            G = a[14],
            y = a[15],
            A = c * j - d * g,
            B = c * o - f * g,
            C = c * q - h * g,
            w = d * o - f * j,
            D = d * q - h * j,
            H = f * q - h * o,
            E = i * z - r * v,
            s = i * G - u * v,
            F = i * y - l * v,
            t = r * G - u * z,
            J = r * y - l * z,
            K = u * y - l * G,
            e = 1 / (A * K - B * J + C * t + w * F - D * s + H * E);
        b[0] = (j * K - o * J + q * t) * e;
        b[1] = (-d * K + f * J - h * t) * e;
        b[2] = (z * H - G * D + y * w) * e;
        b[3] = (-r * H + u * D - l * w) * e;
        b[4] = (-g * K + o * F - q * s) * e;
        b[5] = (c * K - f * F + h * s) * e;
        b[6] = (-v * H + G * C - y * B) * e;
        b[7] = (i * H - u * C + l * B) * e;
        b[8] = (g * J - j * F + q * E) * e;
        b[9] = (-c * J + d * F - h * E) * e;
        b[10] = (v * D - z * C + y * A) * e;
        b[11] = (-i * D + r * C - l * A) * e;
